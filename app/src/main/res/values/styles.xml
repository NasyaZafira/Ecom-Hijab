        <item name="android:textSize">14sp</item>
    </style>

    <style name="Button"/>

    <style name="Button.Primary">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">#202020</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/shape_rectangle_8_ffc700</item>
    </style>

    <style name="Button.Secondary">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">@color/colorBase</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/shape_rectangle_8_8d92a3</item>
    </style>

    <style name="Button.Cancelled">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/shape_rectangle_8_d9435e</item>
    </style>

    <style name="Tab">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">#8D92A3</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="RatingBar">
        <item name="colorControlNormal">#ECECEC</item>
        <item name="colorControlActivated">#FA95B1</item>
    </style>

    <style name="TabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">14sp</item>
    </style>

</resources>